{
    "name": "anatolia-fulfillment/client",
    "type": "package",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AnatoliaFulfillment\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnatoliaFulfillment\\Client\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.6 || ^7.7 || ^7.8",
        "laravel/helpers": "^1.6",
        "laravel/framework": "^9.0 || ^10.0 || ^11.0"
    },
    "authors": [
        {
            "name": "Ahmet Barut",
            "email": "ahmetbarut588@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "AnatoliaFulfillment\\Client\\FulfillmentClientServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "symfony/var-dumper": "^6.3"
    }
}
